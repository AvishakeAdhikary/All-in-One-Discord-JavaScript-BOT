  "description": "An All-in-One JavaScript Discord BOT",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start:dashboard": "cd dashboard && npm run dev",
    "start:server": "node .",
    "start": "concurrently \"npm run start:dashboard\" \"npm run start:server\"",
